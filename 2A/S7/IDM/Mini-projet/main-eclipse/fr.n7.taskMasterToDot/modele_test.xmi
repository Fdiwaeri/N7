<?xml version="1.0" encoding="UTF-8"?>
<taskmaster:EventBundle
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:taskmaster="http://taskmaster"
    name="LancementProduit">
  
  <supplies xsi:type="taskmaster:Supply" consumable="false" name="Serveur" available="5"/>
  
  <supplies xsi:type="taskmaster:Supply" consumable="true" name="Doc_Papier" available="10"/>
  
  <events xsi:type="taskmaster:Event" name="Conception_start"/>
  
  <events xsi:type="taskmaster:Event" name="Conception_finish">
    <requirements xsi:type="taskmaster:Causal" target="//@events.0"/>
  </events>
  
  <events xsi:type="taskmaster:Event" name="Tests_start">
    <requirements xsi:type="taskmaster:Causal" target="//@events.1"/>
    
    <requirements xsi:type="taskmaster:Physical" requested="2" target="//@supplies.0"/>
  </events>
  
  <events xsi:type="taskmaster:Event" name="Deploiement_finish">
    <requirements xsi:type="taskmaster:Causal" target="//@events.2"/>
    
    <requirements xsi:type="taskmaster:Physical" requested="1" target="//@supplies.1"/>
  </events>
  
</taskmaster:EventBundle>